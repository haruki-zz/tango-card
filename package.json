{
  "name": "tango-card",
  "version": "0.1.0",
  "description": "Electron + React + TypeScript flashcard app for Japanese vocabulary.",
  "main": "dist/main/app_entry.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "concurrently -k \"npm:dev:main\" \"npm:dev:renderer\" \"npm:dev:electron\"",
    "dev:main": "tsc --project tsconfig.node.json --watch",
    "dev:renderer": "vite",
    "dev:electron": "wait-on dist/main/app_entry.js http://localhost:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc --project tsconfig.node.json",
    "build:renderer": "vite build",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"{src,tests,docs}/**/*.{ts,tsx,md}\"",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haruki-zz/tango-card.git"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "japanese",
    "flashcards"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/haruki-zz/tango-card/issues"
  },
  "homepage": "https://github.com/haruki-zz/tango-card#readme",
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^38.4.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3",
    "vite": "^7.1.11",
    "wait-on": "^9.0.1"
  }
}
